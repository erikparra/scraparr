{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "title": "Scraparr",
      "name": "scraparr",
      "categories": [
        "Monitoring"
      ],
      "description": "Scraparr is a Prometheus Exporter for various components of the *arr Suite",
      "logo": "https://raw.githubusercontent.com/thecfu/scraparr/refs/heads/main/.github/assets/logos/scraparr_logo.png",
      "platform": "linux",
      "image": "ghcr.io/thecfu/scraparr:latest",
      "ports": [
        "7100/tcp"
      ],
      "volumes": [
        {
          "bind": "/portainer/Files/AppData/Scraparr/config",
          "container": "/scraparr/config"
        }
      ]
    }
  ]
}
